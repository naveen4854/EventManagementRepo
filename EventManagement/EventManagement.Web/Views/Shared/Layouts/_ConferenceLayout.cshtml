@using EventManagement.Notifications;
<!DOCTYPE html>

<html>

<head>
    <script type="text/javascript" src="@Url.Content("~/Content/js/jquery.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/bootstrap.min.js")"></script>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?key=AIzaSyAjQWaXbx2e7aCm4nVaJEFnrGTp4MQAaDU"></script>
    @*<script type="text/javascript" src="@Url.Content("~/Content/js/gmaps.js")"></script>*@
    <script type="text/javascript" src="@Url.Content("~/Content/js/smoothscroll.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/jquery.parallax.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/coundown-timer.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/jquery.scrollTo.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/jquery.nav.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/main.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/scripts/jquery.validate.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/scripts/jquery.validate.unobtrusive.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/scripts/toastr.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/aos.js")"></script>

    <link href="@Url.Content("~/Content/css/bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css/font-awesome.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css/main.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css/animate.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css/responsive.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css/pushy-buttons.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css/toastr.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css/aos.css")" rel="stylesheet">

    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="@Url.Content("~/Content/images/ico/apple-touch-icon-144-precomposed.png")" )">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="@Url.Content("~/Content/images/ico/apple-touch-icon-114-precomposed.png")" )">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="@Url.Content("~/Content/images/ico/apple-touch-icon-72-precomposed.png")" )">
    <link rel="apple-touch-icon-precomposed" href="@Url.Content("~/Content/images/ico/apple-touch-icon-57-precomposed.png")" )">

    <link rel="icon" type="image/png" href="@Url.Content("~/Content/images/fevi_logo.png")">
    <link rel="icon" href="http://example.com/favicon.png">

    <title>Cognizance Scientific - Conference</title>
</head>
<body>
    <div id="mvcNotify" style="display: none">
        @Html.RenderMessages()
    </div>
    <div class="overlay">
        <div class="spinner"></div>
    </div>
    <header id="header" role="banner">
        <div class="main-nav">
            <div class="container">
                <div class="header-top">
                    <div class="pull-right social-icons">
                        <a href="#"><i class="fa fa-twitter"></i></a>
                        <a href="#"><i class="fa fa-facebook"></i></a>
                        <a href="#"><i class="fa fa-google-plus"></i></a>
                        <a href="#"><i class="fa fa-youtube"></i></a>
                    </div>
                </div>
                <div class="row">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        @{ Html.RenderAction("Logo", "Shared"); }
                    </div>
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-right">
                            <li class="active"><a class="scroll homeicon" data-scroll="#Home" data-noscroll="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+"/Home#Home")" href="#Home"><i style="" class="fa fa-home" aria-hidden="true"></i></a></li>
                            <li class="dropdown">
                                <a class="no-scroll pointer">General info</a>
                                <div class="dropdown-content">
                                    <a class="no-scroll" href="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+ "/OrganisingCommitte/")">Organising Committe</a>
                                    <a class="no-scroll" href="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+ "/KeynoteSpeakers/")">Keynote Speakers</a>
                                    <a class="no-scroll" href="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+ "/ScientificSessions/")">Scientific Sessions</a>
                                    <a class="no-scroll" href="@Url.Content("~/Conference/SponsorExhibitor/")">Sponsor/Exhibitor</a>
                                </div>
                            </li>
                            <li><a class="no-scroll" href="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+ "/AbstractSubmit/")">Abstract Submission</a></li>
                            <li class="dropdown">
                                <a class="no-scroll pointer">Registration</a>
                                <div class="dropdown-content">
                                    <a class="no-scroll" href="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+ "/Registration/")">Register Now</a>
                                    <a class="no-scroll" href="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+ "/RegistrationPolicy/")">Registration policy</a>
                                </div>
                            </li>
                            <li class="dropdown">
                                <a class="no-scroll pointer">Agenda</a>
                                <div class="dropdown-content">
                                    <a class="no-scroll" href="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+ "/Program/")">Program</a>
                                    <a class="no-scroll" href="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+ "/Poster/")">Poster</a>
                                    @*<a class="no-scroll" href="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+ "/DownloadBrochure")">Brochure</a>*@
                                    <a class="no-scroll" href="">Brochure</a>
                                </div>
                            </li>
                            <li><a class="scroll" data-noscroll="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+"/Home#venue")"  href="#venue">Venue</a></li>
                            <li><a class="no-scroll" href="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+ "/Guidelines/")">Guidelines</a></li>
                            <li><a class="scroll force-scroll" data-noscroll="@Url.Content("~/Conference/"+@ViewData["Conferencekey"]+"/Home#Contact")" href="#Contact">Contact</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!--/#header-->
    <div>
        @RenderBody()
    </div>
    <script>
    AOS.init();
    </script>
    <footer id="footer" style="padding:35px 0 0 0">
        <section id="Contact">
            <div class="container">
                <div class="footer-content">
                    <div class="row">
                        <div class="text-center">
                            <div id="confContact">
                                <div class="contactTitle defaultcolor">CONTACT US</div>
                                <div><strong>Cognizance Scientific</strong><br></div>
                                <div style="padding-left: 46px;">
                                    27 Old Gloucester Street,
                                    LONDON,
                                    WC1N 3AX,
                                    UNITED KINGDOM
                                </div>
                                <div><i class="fa fa-phone" aria-hidden="true"> &nbsp;+441315102006</i></div>
                                <div><i class="fa fa-envelope" aria-hidden="true"> &nbsp;@{ Html.RenderAction("ConferenceEmail", "Conference", new { key = @ViewData["Conferencekey"] }); } </i></div>
                                <p></p>
                            </div>
                            <p style="text-transform:uppercase"><span>©</span> Copyright 2017. <span class="defaultcolor">scientificcognizance.com </span>All Right Reserved.</p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
</footer>
    <!--/#footer-->
</body>

</html>
